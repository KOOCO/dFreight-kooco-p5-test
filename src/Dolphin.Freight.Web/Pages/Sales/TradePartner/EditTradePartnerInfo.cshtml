            <h4>@L["Display:TradePartner:H4:OtherSetting"]</h4>
            <hr />
            <abp-row>
                <abp-column>
                    <abp-input asp-for="TPInfoModel.IsActive" />
                </abp-column>
                <abp-column>

                </abp-column>
                <abp-column>

                </abp-column>
                <abp-column>

                </abp-column>
            </abp-row>
